algoritmo "ApenasC"
var
   nome: Caracter
   SC: vetor [1..10] de Caractere
   c, t: Inteiro
inicio
   Para c <- 1 ate 10 faca
     Escreva(" Digite seu nome: ")
     Leia(nome)
     Se (copia(maiusc(nome), 1, 1) = "C") entao
       t <- t + 1
       sc[t] <- nome
     FimSe
   FimPara
   
   LimpaTela
   EscrevaL(" LISTAGEM FINAL ")
   Escreval("----------------")
   Para c <- 1 ate t faca
     EscrevaL(sc[c])
   FimPara
fimalgoritmo