algoritmo "ConversorMoedas"
var
 reais, dolares: Real
 C, N: Inteiro

inicio
 C <- 1
 Escreva("Quantas vezes precisa converter? ")
 Leia (N)
 Enquanto (C <= N) faca
    Escreva("Quantos Reais eu tenho? R$ ")
    Leia (reais)
    dolares <- reais/4.99
    Escreva("Posso ter US$", dolares:5:2)
    C <- C + 1
 FimEnquanto

fimalgoritmo