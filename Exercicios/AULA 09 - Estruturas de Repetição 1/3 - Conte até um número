algoritmo "ConteAte?"

var
   Contador, N, S: Inteiro
inicio
      Contador <- 0
      Escreva("Quer contar atÃ© quanto? ")
      Leia (N)
      Escreva("Qual o valor do salto? ")
      Leia(S)
      Enquanto (Contador <= N) faca
