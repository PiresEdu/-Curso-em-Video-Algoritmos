algoritmo "QuaisEntre0e10"

var
  C, V, T, I: Inteiro
inicio
   Para C <- 1 ate 6 faca
      Escreva(" Digite um valor: ")
      Leia(V)
      Se (V >= 0) e (V <=10) entao
        T <- T + 1
      FimSe
      Se (V % 2 = 1) entao
          I <- V + I
      FimSe
   FimPara
   EscrevaL(" Ao total obtemos: ", T, " números entre 1 e 10")
   EscrevaL(" A soma de todos os Ímpares é ", I)
fimalgoritmo